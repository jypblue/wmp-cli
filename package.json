  "description": "a wmp-cli project for wechat mini program",
  "main": "bin/index.js",
  "bin": {
    "wmp": "bin/wmp"
  },
  "scripts": {
    "test:serve": "cd test && ENV=development CONF=dev ../bin/wmp serve",
    "test:build": "cd test && ENV=development CONF=build GENERATE_SOURCEMAP=false ../bin/wmp build",
    "prebuild": "rimraf ./lib && mkdirp ./lib",
    "prepublish": "npm run build",
    "build:watch": "babel src -o lib/index.js -w",
    "build": "npx babel src --out-dir lib",
    "lint": "eslint --ext .js command/ --fix"
