    "prebuild": "rimraf ./lib && mkdirp ./lib",
    "prepublish": "npm run build",
    "submit": "npm publish --access public",
    "build:watch": "babel src -o lib/index.js -w",
    "build": "npx babel src --out-dir lib",
    "lint": "eslint --ext .js command/ --fix"
  },
